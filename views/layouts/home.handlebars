<!DOCTYPE html>
<html lang="en">

  <head>
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-99CCFS6DEQ"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'G-99CCFS6DEQ');
    </script>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="format-detection" content="telephone=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="author" content="Postidal">
    <meta name="keywords" content="{{metaData.keywords}}">
    <meta name="description" content="{{metaData.description}}">
    <title>{{metaData.title}}</title>

    <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "WebSite",
      "url": "http://localhost:8080/",
      "potentialAction": {
        "@type": "SearchAction",
        "target": "https://localhost:8080/shop?search={search_term_string}",
        "query-input": "required name=search_term_string"
      }
    }
    </script>

    <link href="https://fonts.googleapis.com/css?family=Work+Sans:300,400,500,600,700&amp;amp;subset=latin-ext" rel="stylesheet">
    <link rel="stylesheet" href="{{config.root}}fonts/Linearicons/Linearicons/Font/demo-files/demo.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="{{config.root}}plugins/owl-carousel/assets/owl.carousel.css">
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css"/>

<link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick-theme.css"/>
    <link rel="stylesheet" href="{{config.root}}plugins/lightGallery-master/dist/css/lightgallery.min.css">
    <link rel="stylesheet" href="{{config.root}}plugins/jquery-bar-rating/dist/themes/fontawesome-stars.css">
    <link rel="stylesheet" href="{{config.root}}plugins/jquery-ui/jquery-ui.min.css">
    <link rel="stylesheet" href="{{config.root}}plugins/select2/dist/css/select2.min.css">
    <link rel="stylesheet" href="{{config.root}}css/style.css">
    <link rel="stylesheet" href="{{config.root}}css/electronic.css">
    <style>
      #mainContainer{
        opacity:0;
      }
    </style>
    <script src="https://code.jquery.com/jquery-1.12.4.min.js"></script>
  </head>

  <body onload="loadContent()">

    <div id="mainContainer">

      {{>home/home-nav}}

        {{#if success_message}}
          <div class="alert alert-success alert-dismissible fade show mt-2 text-center">
            <button class="close" data-dismiss="alert" type="button">
                <span>&times;</span>
            </button>
            <strong style="font-size:1.7rem;">{{success_message}}</strong>
          </div>
        {{/if}}

        {{#if error_message}}
            <div class="alert alert-danger text-center">
                <strong style="font-size:1.7rem;">{{error_message}}</strong>
            </div>
        {{/if}}

    	{{{body}}}

      {{>home/footer}}

    </div>
  </body>

</html>
<script src="{{config.root}}plugins/owl-carousel/owl.carousel.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"></script>
