}
  resetToDefaults()
    topbar.show()
  function loadContent(){
      $("#mainContainer").css("opacity",1);
      topbar.hide()
  }

  $(".slider").slick({

            // normal options...
            infinite: true,
            arrows:true,
            autoplay:true,
            autoplaySpeed:5000,
            centerMode:true,
            slidesToShow: 1,
            });
</script>