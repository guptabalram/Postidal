                                            {{/each}}
                                        </ul> --}}
                                        {{!-- {{/if}} --}}
                                    </li>
                                    {{/each}}
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-9 col-lg-12 col-md-12 col-sm-12 col-12 ">
                        <div class="ps-block--categories-grid" data-mh="catalog-top" style="height: 393px;">
                            {{#each headerCategories as |cat indexCat|}}
                            <div class="ps-block--category-2" data-mh="categories" style="height: 209px;">
                                <div class="ps-block__thumbnail"><img src="{{cat.image}}" alt="" class="imgCat"></div>
                                <div class="ps-block__content">
                                    <h4>{{cat.name}}</h4>
                                    {{#if subcat.length}}
                                    <ul>
                                        {{#each subcat as |sub indexSub|}}
                                        <li><a href="{{config.root}}shop?page=1&subcategory={{sub._id}}">{{sub.name}}</a></li>
                                        {{/each}}
                                    </ul>
                                    {{/if}}
                                    <a href="/{{config.root}}/category/{{cat.slug}}">Shop All</a>
                                </div>
                            </div>
                            {{/each}}
                        </div>
                    </div>
                </div>
            </div>

